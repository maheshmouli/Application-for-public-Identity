<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Alogin</servlet-name>
        <servlet-class>login.Alogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ulogin</servlet-name>
        <servlet-class>login.Ulogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reg</servlet-name>
        <servlet-class>login.Reg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload1</servlet-name>
        <servlet-class>Upload1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload2</servlet-name>
        <servlet-class>Upload2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Alogin</servlet-name>
        <url-pattern>/Alogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ulogin</servlet-name>
        <url-pattern>/Ulogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reg</servlet-name>
        <url-pattern>/Reg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/Upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload1</servlet-name>
        <url-pattern>/Upload1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload2</servlet-name>
        <url-pattern>/Upload2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
